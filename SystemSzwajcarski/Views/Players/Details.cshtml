@model SystemSzwajcarski.Models.RelationOP
@{
    ViewData["Title"] = "MyAccount";
}
<h1>Moje konto</h1>
<p>Imie: @Model.Player.Name</p>
<p>Nazwisko :@Model.Player.LastName</p>
<p>Login: @Model.Player.Login</p>
<p>Email: @Model.Player.Email</p>
<p>Ranking:  @Model.Ranking</p>
<a href="@Url.Action("Viewdata","Players", new {Id=Model.idRelation})">Modyfikuj</a>
<a href="@Url.Action("DelatePlayer","Players", new {Id=Model.idRelation})">Usuń</a>


